type User = 
 record {
   created_at: opt int;
   email: text;
   phone_number: text;
   updated_at: opt int;
   username: text;
 };
type Result_1 = 
 variant {
   err: Error;
   ok: User;
 };
type Result = 
 variant {
   err: Error;
   ok;
 };
type Error = 
 variant {
   AlreadyExisting;
   NotAuthorized;
   NotFound;
 };
service : {
  createUser: (User) -> (Result);
  deleteUser: () -> (Result);
  readUser: () -> (Result_1);
  updateUser: (User) -> (Result);
}
